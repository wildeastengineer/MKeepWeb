{
    "name": "mkeepweb1",
    "version": "0.1.0",
    "description": "Web client for MoneyKeeper",
    "main": "index.js",
    "scripts": {
        "start": "npm run build && NODE_ENV='production' NODE_PATH=./client ./node_modules/.bin/babel-node --presets es2015,react,stage-0 server/server.js",
        "dev": "NODE_ENV='development' BUILDING='true' webpack-dev-server --config webpack.config.js --progress --watch --hot",
        "build": "NODE_ENV='production' BUILDING='true' webpack --config webpack.config.js",
        "build-help": "NODE_ENV='production' BUILDING='true' webpack --config webpack.config.js --display-error-details",
        "test": "jest",
        "nodemon": "NODE_PATH=./client nodemon server/server.js --exec babel-node --presets es2015,react,stage-0",
        "server": "NODE_PATH=./client nodemon server/server.js --ignore client/ --exec babel-node --presets es2015,react,stage-0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wildeastengineer/MKeepWeb.git"
    },
    "author": "MK team",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/wildeastengineer/MKeepWeb/issues"
    },
    "homepage": "https://github.com/wildeastengineer/MKeepWeb#readme",
    "dependencies": {
        "autoprefixer": "^6.7.7",
        "babel-cli": "^6.24.0",
        "babel-core": "^6.24.0",
        "babel-eslint": "^7.2.3",
        "babel-jest": "^19.0.0",
        "babel-loader": "^6.4.0",
        "babel-preset-es2015": "^6.24.0",
        "babel-preset-react": "^6.23.0",
        "babel-preset-stage-0": "^6.22.0",
        "babel-preset-stage-2": "^6.24.1",
        "clean-webpack-plugin": "^0.1.16",
        "cookie-parser": "^1.4.3",
        "css-loader": "^0.27.3",
        "ejs": "^2.5.6",
        "eslint": "^3.19.0",
        "eslint-loader": "^1.7.1",
        "eslint-plugin-react": "^7.0.1",
        "express": "^4.15.2",
        "extract-text-webpack-plugin": "^2.1.0",
        "file-loader": "^0.11.1",
        "jest-cli": "^19.0.2",
        "lodash": "^4.17.4",
        "node-sass": "^4.5.0",
        "postcss-loader": "^1.3.3",
        "prop-types": "^15.5.10",
        "react": "^15.4.2",
        "react-dom": "^15.4.2",
        "react-hot-loader": "^3.0.0-beta.6",
        "react-redux": "^5.0.3",
        "react-router": "^3.0.0",
        "react-router-redux": "^4.0.8",
        "redux": "^3.6.0",
        "redux-devtools-extension": "^2.13.0",
        "redux-thunk": "^2.2.0",
        "sass-loader": "^6.0.3",
        "serve-favicon": "^2.4.3",
        "style-loader": "^0.14.1",
        "superagent": "^3.5.2",
        "url-loader": "^0.5.8",
        "webpack": "^2.2.1",
        "webpack-dev-server": "^2.4.2"
    }
}
